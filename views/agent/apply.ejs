<!-- BSC DOCUMENTS -->
<form action="/agent/submit/bsc/<%= user.id %>" method="POST" enctype="multipart/form-data">
    <div class="row" id="pgd">
        <div class="text-center color"> <h5> Bsc / Certificate program required documents</h5> 
            <small class="text-danger">All files must be in colored PDF format </small>
        </div>
       

        <div class="col-md-4 col-12">
            <div class="input-field choice">
                <div>
                    <div class="input-group mb-4">
                        <label for="other" class="form-control-label">O-Level Result  </label>
                    </div>
                    <div class="input-group mb-4">
                        <input type="file"    class="form-control" name="waec" required>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4 col-12">
            <div class="input-field choice">
                <div>
                    <div class="input-group mb-4">
                        <label for="other" class="form-control-label">International Passport Data Page </label>
                    </div>
                    <div class="input-group mb-4">
                        <input type="file"    class="form-control" name="passport" required>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4 col-12">
            <div class="input-field choice">
                <div>
                    <div class="input-group mb-4">
                        <label for="other" class="form-control-label">Other Document </label>
                    </div>
                    <div class="input-group mb-4">
                        <input type="file" class="form-control" name="other">
                    </div>
                </div>
            </div>
        </div>


       
   
    </div>

    <div class="row">
        <div class="text-center color"> <span> Course of study</span> 
